{
    "name": "wsr/booking",
    "type": "typo3-cms-extension",
    "description": "Booking extension for FE users",
    "authors": [
        {
            "name": "Joachim Ruhs",
            "role": "Developer"
        }
    ],
	"license": "MIT",	
    "require": {
        "typo3/cms-core": "^11 || ^12" 
    },
    "autoload": {
        "psr-4": {
            "WSR\\Booking\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WSR\\Booking\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/booking": "self.version"
    },
    "extra": {
      "typo3/cms": {
         "extension-key": "booking"
      }
   }	
}